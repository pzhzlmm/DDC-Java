package com.atguigu.ex7;

/**   
 * @ClassName:  GeometricObject   
 * @Description:TODO  
 * @author: XinLan Wang
 * @date:   2020年2月29日 下午2:50:33   
 *     
 */
public class GeometricObject {
	String color;
	double weight;
	/**
	 * @param color
	 * @param weight
	 */
	public GeometricObject(String color, double weight) {
		super();
		this.color = color;
		this.weight = weight;
	}
	
}
