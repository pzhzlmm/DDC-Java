
package ex6;
public class BankTest{
	public static void main(String[] args) {
		
	}
}
class Bank {
	private int account;
	private String password;
	private int balance;
	static int count = 1000;
	final static private float INTEREST = 0.03f;
	final static private int MINIMUMBALANCE = 1000;

	/**
	 * 
	 */
	public BankTest() {
		super();
		this.setAccount(count);
	}

	/**
	 * @param account
	 * @param password
	 * @param balance
	 */
	public BankTest(int account, String password, int balance) {
		super();
		this.account = account;
		this.password = password;
		this.balance = balance;
	}

	/**
	 * @return the account
	 */
	public int getAccount() {
		return account;
	}

	/**
	 * @param account the account to set
	 */
	private void setAccount(int account) {
		this.account = account;
		count++;
	}

	/**
	 * @return the password
	 */
	public String getPassword() {
		return password;
	}

	/**
	 * @param password the password to set
	 */
	public void setPassword(String password) {
		this.password = password;
	}

	/**
	 * @return the balance
	 */
	public int getBalance() {
		return balance;
	}

	/**
	 * @param balance the balance to set
	 */
	public void setBalance(int balance) {
		this.balance = balance;
	}

	/**
	 * @return the interest
	 */
	public static float getInterest() {
		return INTEREST;
	}

	/**
	 * @return the minimumbalance
	 */
	public static int getMinimumbalance() {
		return MINIMUMBALANCE;
	}

}



