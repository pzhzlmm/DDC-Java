package com.atguigu;

/**   
 * @ClassName:  GeometricObject   
 * @Description:TODO  
 * @author: XinLan Wang
 * @date:   2020年2月29日 下午9:03:30   
 *     
 */
public class GeometricObject {
	protected String color;
	protected double weight;
	/**
	 * 
	 */
	public GeometricObject() {
		super();
	}
	/**
	 * @param color
	 * @param weight
	 */
	public GeometricObject(String color, double weight) {
		super();
		this.color = color;
		this.weight = weight;
	}
	
}
