 */
public class StudentTest {
	public static void main(String[] args) {
		Vector<Student> v = new Vector<>();

		for (int i = 0; i < 30; i++) { 
			v.addElement(new Student
			(i,(int)(Math.random() * 50 + 50)));
		}
		
		System.out.println(v.elementAt(0));
		System.out.println(v.size());
		
	}
}
