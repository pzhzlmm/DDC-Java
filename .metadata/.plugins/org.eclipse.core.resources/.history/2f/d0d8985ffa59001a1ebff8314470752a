package com.atguigu.ex.ex3;

/**   
 * @ClassName:  Cylinder   
 * @Description:TODO  
 * @author: XinLan Wang
 * @date:   2020年2月28日 下午3:15:03   
 *     
 */
public class Cylinder extends Circle{
	double length;

	/**
	 * 
	 */
	public Cylinder() {
		super();
		length = 1;
	}

	/**
	 * @return the length
	 */
	public double getLength() {
		return length;
	}

	/**
	 * @param length the length to set
	 */
	public void setLength(double length) {
		this.length = length;
	}
	
	public double findVolume(){
		return this.findArea() * length;
	}
	
}
